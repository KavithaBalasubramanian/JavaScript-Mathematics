//Given a number N in decimal convert it into binary value.


const readline = require('readline');

const inp = readline.createInterface({
  input: process.stdin
});

//const userInput = [];

inp.on("line", (Data) => {
    var inputData = Data;
    var a=parseInt(inputData);
    var c= parseInt(a,10).toString(2);
    console.log(c)

});
