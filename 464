//Given an integer N, find if it is divisible by 11


const readline = require('readline');

const inp = readline.createInterface({
  input: process.stdin
});

//const userInput = [];

inp.on("line", (Data) => {
    var inputData = Data;
    var n =parseInt(Data);
  if (n%11===0)
  console.log("YES")
  else
    console.log("NO")
});
    
