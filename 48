//


const readline = require('readline');

const inp = readline.createInterface({
  input: process.stdin
});

//const userInput = [];

inp.on("line", (Data) => {
    var inputData = Data;
    var n =parseInt(Data);
  var fact = 1;
    for(i=1;i<=n;i++)
    {
      fact=fact*i;
    }
  console.log(fact);
});
 
