//Given a number N in binary format convert it to decimal number.



const readline = require('readline');

const inp = readline.createInterface({
  input: process.stdin
});

//const userInput = [];

inp.on("line", (Data) => {
    var inputData = Data;
    var a=parseInt(inputData);
    var c= parseInt(a,2).toString(10);
    console.log(c)

});
