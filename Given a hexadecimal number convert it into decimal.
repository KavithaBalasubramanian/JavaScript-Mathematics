//Given a hexadecimal number convert it into decimal.


const readline = require('readline');

const inp = readline.createInterface({
  input: process.stdin
});

//const userInput = [];

inp.on("line", (Data) => {
    var inputData = Data;
    var a=(inputData);
    var c= parseInt(a,16).toString(10);
    console.log(c)

});
    
