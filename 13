//Given 2 numbers N,M. Print 'yes' if their product is a perfect square else print 'no'.

const readline = require('readline');

const inp = readline.createInterface({
  input: process.stdin
});

//const userInput = [];

inp.on("line", (Data) => {
   var inputData = Data.split(" ");
    var n =parseInt(inputData[0]);
    var m =parseInt(inputData[1]);
    var c = n*m;
    var a = Math.sqrt(c);
    var b = parseInt(a)*parseInt(a);
  if(c===b)
    
        console.log("yes")
        else
        console.log("no")
    

});
